{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["astro/client","react", "react-dom"],
    "moduleResolution": "node",
    "isolatedModules": true,
    "module": "ESNext",
    "target": "ESNext"
  }
}